{
  "name": "gatsby-source-kdc-cms",
  "version": "0.1.3",
  "description": "Gatsby source plugin for building websites using KDC CMS as a data source",
  "author": {
    "name": "Ian Dela Cruz",
    "email": "ian@kdcsoftware.com",
    "url": "https://www.kdcsoftware.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ianpogi5/gatsby-source-kdc-cms.git"
  },
  "bugs": {
    "url": "https://github.com/ianpogi5/gatsby-source-kdc-cms/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/ianpogi5/kdc-cms",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir .",
